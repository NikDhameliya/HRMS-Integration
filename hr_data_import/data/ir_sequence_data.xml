<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data noupdate="1">

        <record id="seq_employee_queue_data" model="ir.sequence">
            <field name="name">HRMS Employee Queue Data Sequence</field>
            <field name="code">employee.data.queue</field>
            <field name="prefix">EMPQ</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_department_queue_data" model="ir.sequence">
            <field name="name">HRMS Department Data Queue Sequence</field>
            <field name="code">department.data.queue</field>
            <field name="prefix">DPQ</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_synced_hrms_leave_sequence" model="ir.sequence">
            <field name="name">HRMS Leave Data Queue Sequence</field>
            <field name="code">leave.data.queue</field>
            <field name="prefix">LEAVQ</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>


    </data>
</odoo>
